<DataValueMember DataType="BOOL" Name="CVLimitsFault" Value="0"/>
<DataValueMember DataType="BOOL" Name="CVInitFault" Value="0"/>
<DataValueMember DataType="BOOL" Name="EUSpanChanged" Value="0"/>
<DataValueMember DataType="BOOL" Name="CVChanged" Value="0"/>
<DataValueMember DataType="BOOL" Name="ATuneTimedOut" Value="0"/>
<DataValueMember DataType="BOOL" Name="PVNotSettled" Value="0"/>
<DataValueMember DataType="BOOL" Name="PVChangeTooSmall" Value="0"/>
<DataValueMember DataType="BOOL" Name="StepSizeTooSmall" Value="0"/>
<DataValueMember DataType="BOOL" Name="GainTooLarge" Value="0"/>
