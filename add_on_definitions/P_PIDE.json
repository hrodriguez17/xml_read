inCV; END_IF;\n</CData>\n</Line>\n<Line Number=\"352\">\n<CData>\n\tIF (Inp_OvrdCV &gt; Cfg_MaxCV) THEN Inp_OvrdCV := Cfg_MaxCV; END_IF;\n</CData>\n</Line>\n<Line Number=\"353\">\n<CData>\n\tIF (Inp_OvrdCV &lt; Cfg_MinCV) THEN Inp_OvrdCV := Cfg_MinCV; END_IF;\n</CData>\n</Line>\n<Line Number=\"354\">\n<CData>\nEND_IF;\n</CData>\n</Line>\n<Line Number=\"355\">\n<CData>\n// ##### V3.5-02: Copy CV to CVOper when loop is in init.\n</CData>\n</Line>\n<Line Number=\"356\">\n<CData>\nIF (Inp_UseCVInitVal) THEN\n</CData>\n</Line>\n<Line Number=\"357\">\n<CData>\n\tOSet_CV := Val_CVOut;\n</CData>\n</Line>\n<Line Number=\"358\">\n<CData>\nEND_IF;\n</CData>\n</Line>\n<Line Number=\"359\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"360\">\n<CData>\n// Deviation Limits Settings Tracking\n</CData>\n</Line>\n<Line Number=\"361\">\n<CData>\nIF (Cfg_SetTrack) THEN\n</CData>\n</Line>\n<Line Number=\"362\">\n<CData>\n\tPSet_HiHiDevLim := Val_HiHiDevLim;  OSet_HiHiDevLim := Val_HiHiDevLim;\n</CData>\n</Line>\n<Line Number=\"363\">\n<CData>\n\tPSet_HiDevLim   := Val_HiDevLim;    OSet_HiDevLim   := Val_HiDevLim;\n</CData>\n</Line>\n<Line Number=\"364\">\n<CData>\n\tPSet_LoDevLim   := Val_LoDevLim;    OSet_LoDevLim   := Val_LoDevLim;\n</CData>\n</Line>\n<Line Number=\"365\">\n<CData>\n\tPSet_LoLoDevLim := Val_LoLoDevLim;  OSet_LoLoDevLim := Val_LoLoDevLim;\n</CData>\n</Line>\n<Line Number=\"366\">\n<CData>\nELSE\n</CData>\n</Line>\n<Line Number=\"367\">\n<CData>\n// ##### V3.0-04: Corrected assignments in following 8 THENs.\n</CData>\n</Line>\n<Line Number=\"368\">\n<CData>\n\tIF (PSet_HiHiDevLim &lt; 0.0) THEN PSet_HiHiDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"369\">\n<CData>\n\tIF (PSet_HiDevLim &lt; 0.0) THEN PSet_HiDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"370\">\n<CData>\n// ##### V3.0-02: Reversed sense of Lo, LoLo Deviation Limits.\n</CData>\n</Line>\n<Line Number=\"371\">\n<CData>\n\tIF (PSet_LoDevLim &gt; 0.0) THEN PSet_LoDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"372\">\n<CData>\n\tIF (PSet_LoLoDevLim &gt; 0.0) THEN PSet_LoLoDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"373\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"374\">\n<CData>\n\tIF (OSet_HiHiDevLim &lt; 0.0) THEN OSet_HiHiDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"375\">\n<CData>\n\tIF (OSet_HiDevLim &lt; 0.0) THEN OSet_HiDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"376\">\n<CData>\n// ##### V3.0-02: reverse sense of Lo, LoLo Deviation Limits.\n</CData>\n</Line>\n<Line Number=\"377\">\n<CData>\n\tIF (OSet_LoDevLim &gt; 0.0) THEN OSet_LoDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"378\">\n<CData>\n\tIF (OSet_LoLoDevLim &gt; 0.0) THEN OSet_LoLoDevLim := 0.0; END_IF;\n</CData>\n</Line>\n<Line Number=\"379\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"380\">\n<CData>\nEND_IF;\n</CData>\n</Line>\n<Line Number=\"381\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"382\">\n<CData>\n// Deviation Threshold Status/Gate Processing\n</CData>\n</Line>\n<Line Number=\"383\">\n<CData>\nHiHiDevGate.Inp := Wrk_PIDE.DevHHAlarm;\n</CData>\n</Line>\n<Line Number=\"384\">\n<CData>\nP_Gate(HiHiDevGate);\n</CData>\n</Line>\n<Line Number=\"385\">\n<CData>\nSts_HiHiDev := HiHiDevGate.Out;\n</CData>\n</Line>\n<Line Number=\"386\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"387\">\n<CData>\nHiDevGate.Inp := Wrk_PIDE.DevHAlarm;\n</CData>\n</Line>\n<Line Number=\"388\">\n<CData>\nP_Gate(HiDevGate);\n</CData>\n</Line>\n<Line Number=\"389\">\n<CData>\nSts_HiDev := HiDevGate.Out;\n</CData>\n</Line>\n<Line Number=\"390\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"391\">\n<CData>\nLoDevGate.Inp := Wrk_PIDE.DevLAlarm;\n</CData>\n</Line>\n<Line Number=\"392\">\n<CData>\nP_Gate(LoDevGate);\n</CData>\n</Line>\n<Line Number=\"393\">\n<CData>\n// ##### V3.0-05: Corrected Low Dev status handling.\n</CData>\n</Line>\n<Line Number=\"394\">\n<CData>\nSts_LoDev := LoDevGate.Out;\n</CData>\n</Line>\n<Line Number=\"395\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"396\">\n<CData>\nLoLoDevGate.Inp := Wrk_PIDE.DevLLAlarm;\n</CData>\n</Line>\n<Line Number=\"397\">\n<CData>\nP_Gate(LoLoDevGate);\n</CData>\n</Line>\n<Line Number=\"398\">\n<CData>\nSts_LoLoDev := LoLoDevGate.Out;\n</CData>\n</Line>\n<Line Number=\"399\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"400\">\n<CData>\n// Alarm Processing\n</CData>\n</Line>\n<Line Number=\"401\">\n<CData>\nHiHiDev.Cfg_PCmdClear := Cfg_PCmdClear;   HiHiDev.PCfg_AllowExist := 1;\n</CData>\n</Line>\n<Line Number=\"402\">\n<CData>\nHiDev.Cfg_PCmdClear := Cfg_PCmdClear;     HiDev.PCfg_AllowExist := 1;\n</CData>\n</Line>\n<Line Number=\"403\">\n<CData>\nLoDev.Cfg_PCmdClear := Cfg_PCmdClear;     LoDev.PCfg_AllowExist := 1;\n</CData>\n</Line>\n<Line Number=\"404\">\n<CData>\nLoLoDev.Cfg_PCmdClear := Cfg_PCmdClear;   LoLoDev.PCfg_AllowExist := 1;\n</CData>\n</Line>\n<Line Number=\"405\">\n<CData>\nIntlkTrip.Cfg_PCmdClear := Cfg_PCmdClear; IntlkTrip.PCfg_AllowExist := 1;\n</CData>\n</Line>\n<Line Number=\"406\">\n<CData>\nFail.Cfg_PCmdClear := Cfg_PCmdClear;      Fail.PCfg_AllowExist := 1;\n</CData>\n</Line>\n<Line Number=\"407\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"408\">\n<CData>\nP_Alarm(HiHiDev);\n</CData>\n</Line>\n<Line Number=\"409\">\n<CData>\nP_Alarm(HiDev);\n</CData>\n</Line>\n<Line Number=\"410\">\n<CData>\nP_Alarm(LoDev);\n</CData>\n</Line>\n<Line Number=\"411\">\n<CData>\nP_Alarm(LoLoDev);\n</CData>\n</Line>\n<Line Number=\"412\">\n<CData>\nP_Alarm(IntlkTrip);\n</CData>\n</Line>\n<Line Number=\"413\">\n<CData>\nP_Alarm(Fail);\n</CData>\n</Line>\n<Line Number=\"414\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"415\">\n<CData>\nSts_AlmInh := ((HiHiDev.Sts_AlmInh OR HiDev.Sts_AlmInh OR LoDev.Sts_AlmInh) OR (LoLoDev.Sts_AlmInh OR IntlkTrip.Sts_AlmInh OR Fail.Sts_AlmInh));\n</CData>\n</Line>\n<Line Number=\"416\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"417\">\n<CData>\n// Not Ready, Available\n</CData>\n</Line>\n<Line Number=\"418\">\n<CData>\nErr_Timer := HiHiDevGate.Sts_Err OR HiDevGate.Sts_Err OR LoDevGate.Sts_Err OR LoLoDevGate.Sts_Err;\n</CData>\n</Line>\n<Line Number=\"419\">\n<CData>\nErr_Alarm := (HiHiDev.Sts_Err OR HiDev.Sts_Err OR LoDev.Sts_Err or LoLoDev.Sts_Err) OR (Fail.Sts_Err or IntlkTrip.Sts_Err);\n</CData>\n</Line>\n<Line Number=\"420\">\n<CData>\nSts_Err := ((Err_RatioLim AND Sts_Casc AND Cfg_HasRatio) OR Err_SPLim OR Err_PVEU OR Err_CVLim) OR (Err_CVEU OR Err_Timer OR Err_DB OR Err_Alarm);\n</CData>\n</Line>\n<Line Number=\"421\">\n<CData>\n// ##### V3.1-00: Added Not Ready reason bits (Nrdy_xxx)\n</CData>\n</Line>\n<Line Number=\"422\">\n<CData>\nNrdy_NoMode := 0;\n</CData>\n</Line>\n<Line Number=\"423\">\n<CData>\nNrdy_IOFault := Sts_PVBad OR Sts_FFBad OR Sts_HandFdbkBad OR Sts_CVBad;\n</CData>\n</Line>\n<Line Number=\"424\">\n<CData>\nNrdy_Init := Inp_UseCVInitVal AND Cfg_InitToMan;\n</CData>\n</Line>\n<Line Number=\"425\">\n<CData>\nNrdy_CfgErr := Sts_Err;\n</CData>\n</Line>\n<Line Number=\"426\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"427\">\n<CData>\nSts_NotRdy := (Nrdy_Intlk OR Nrdy_IOFault OR Nrdy_Init) OR (Nrdy_Disabled OR Nrdy_CfgErr);\n</CData>\n</Line>\n<Line Number=\"428\">\n<CData>\nSts_Available := Sts_Prog AND NOT Sts_NotRdy;\n</CData>\n</Line>\n<Line Number=\"429\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"430\">\n<CData>\n// Val_Sts Enumeration\n</CData>\n</Line>\n<Line Number=\"431\">\n<CData>\nVal_Sts := 0;\n</CData>\n</Line>\n<Line Number=\"432\">\n<CData>\nIF Sts_Disabled THEN Val_Sts := 33;\n</CData>\n</Line>\n<Line Number=\"433\">\n<CData>\nELSIF Sts_Casc THEN\n</CData>\n</Line>\n<Line Number=\"434\">\n<CData>\n\tIF Cfg_HasRatio THEN\n</CData>\n</Line>\n<Line Number=\"435\">\n<CData>\n\t\tVal_Sts := 4;\n</CData>\n</Line>\n<Line Number=\"436\">\n<CData>\n\tELSE\n</CData>\n</Line>\n<Line Number=\"437\">\n<CData>\n\t\tVal_Sts := 3;\n</CData>\n</Line>\n<Line Number=\"438\">\n<CData>\n\tEND_IF;\n</CData>\n</Line>\n<Line Number=\"439\">\n<CData>\nELSIF Sts_Auto THEN Val_Sts := 2;\n</CData>\n</Line>\n<Line Number=\"440\">\n<CData>\nELSIF Sts_Man THEN Val_Sts := 1;\n</CData>\n</Line>\n<Line Number=\"441\">\n<CData>\nELSE Val_Sts := 0;\n</CData>\n</Line>\n<Line Number=\"442\">\n<CData>\nEND_IF;\n</CData>\n</Line>\n<Line Number=\"443\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"444\">\n<CData>\n// Val_State Enumeration\n</CData>\n</Line>\n<Line Number=\"445\">\n<CData>\nIF Sts_Initializing THEN Val_State := 15;\n</CData>\n</Line>\n<Line Number=\"446\">\n<CData>\nELSIF Sts_Hand THEN Val_State := 14;\n</CData>\n</Line>\n<Line Number=\"447\">\n<CData>\nELSIF Sts_IntlkCV THEN Val_State := 13;\n</CData>\n</Line>\n<Line Number=\"448\">\n<CData>\nELSIF Sts_WindupHi THEN Val_State := 9;\n</CData>\n</Line>\n<Line Number=\"449\">\n<CData>\nELSIF Sts_WindupLo THEN Val_State := 8;\n</CData>\n</Line>\n<Line Number=\"450\">\n<CData>\nELSIF Sts_CVClamped THEN Val_State := 7;\n</CData>\n</Line>\n<Line Number=\"451\">\n<CData>\nELSIF Sts_SPClamped THEN Val_State := 6;\n</CData>\n</Line>\n<Line Number=\"452\">\n<CData>\nELSIF Sts_RatioClamped THEN Val_State := 5;\n</CData>\n</Line>\n<Line Number=\"453\">\n<CData>\nELSIF (Sts_Auto OR Sts_Casc) THEN\n</CData>\n</Line>\n<Line Number=\"454\">\n<CData>\n\tIF (Sts_ZCDBOn) THEN\n</CData>\n</Line>\n<Line Number=\"455\">\n<CData>\n\t\tVal_State := 1;\n</CData>\n</Line>\n<Line Number=\"456\">\n<CData>\n\tELSE\n</CData>\n</Line>\n<Line Number=\"457\">\n<CData>\n\t\tVal_State := 2;\n</CData>\n</Line>\n<Line Number=\"458\">\n<CData>\n\tEND_IF;\n</CData>\n</Line>\n<Line Number=\"459\">\n<CData>\nELSE Val_State := 0;\n</CData>\n</Line>\n<Line Number=\"460\">\n<CData>\nEND_IF;\n</CData>\n</Line>\n<Line Number=\"461\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"462\">\n<CData>\n// Val_Fault Enumeration\n</CData>\n</Line>\n<Line Number=\"463\">\n<CData>\nIF Sts_Err THEN Val_Fault := 34;\n</CData>\n</Line>\n<Line Number=\"464\">\n<CData>\nELSIF Sts_Fail THEN Val_Fault := 32;\n</CData>\n</Line>\n<Line Number=\"465\">\n<CData>\nELSIF Nrdy_Intlk THEN Val_Fault := 29;\n</CData>\n</Line>\n<Line Number=\"466\">\n<CData>\nELSIF Sts_HiHiDev THEN Val_Fault := 28;\n</CData>\n</Line>\n<Line Number=\"467\">\n<CData>\nELSIF Sts_LoLoDev THEN Val_Fault := 27;\n</CData>\n</Line>\n<Line Number=\"468\">\n<CData>\nELSIF Sts_HiDev THEN Val_Fault := 23;\n</CData>\n</Line>\n<Line Number=\"469\">\n<CData>\nELSIF Sts_LoDev THEN Val_Fault := 22;\n</CData>\n</Line>\n<Line Number=\"470\">\n<CData>\nELSE Val_Fault := 0;\n</CData>\n</Line>\n<Line Number=\"471\">\n<CData>\nEND_IF;\n</CData>\n</Line>\n<Line Number=\"472\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"473\">\n<CData>\n// #####V3.1-00: Added SrcQ enums\n</CData>\n</Line>\n<Line Number=\"474\">\n<CData>\nWrk_SrcQ := Inp_PVSrcQ;\n</CData>\n</Line>\n<Line Number=\"475\">\n<CData>\nIF (Inp_PVUncertain AND (Wrk_SrcQ &lt; 16)) THEN Wrk_SrcQ := 16; END_IF;\n</CData>\n</Line>\n<Line Number=\"476\">\n<CData>\nIF (Inp_PVBad AND (Wrk_SrcQ &lt; 32)) THEN Wrk_SrcQ := 34; END_IF;\n</CData>\n</Line>\n<Line Number=\"477\">\n<CData>\nIF (Inp_CVIOFault AND (Wrk_SrcQ &lt; 32)) THEN Wrk_SrcQ := 34; END_IF;\n</CData>\n</Line>\n<Line Number=\"478\">\n<CData>\nSrcQ_IO := Wrk_SrcQ;\n</CData>\n</Line>\n<Line Number=\"479\">\n<CData>\nSrcQ := Wrk_SrcQ;\n</CData>\n</Line>\n<Line Number=\"480\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"481\">\n<CData>\n// Notification Value 0=no alarm, 1=ack/reset reqd, 2=Low/acked, 3=Low/unacked, 4=Med/acked, 5=Med/unacked, 6=High/acked, 7=High/unacked, 8=Urgent/acked, 9=Urgent/undacked\n</CData>\n</Line>\n<Line Number=\"482\">\n<CData>\nWrk_Notify := 0;\n</CData>\n</Line>\n<Line Number=\"483\">\n<CData>\nIF (HiHiDev.Val_Notify&gt;Wrk_Notify) THEN Wrk_Notify := HiHiDev.Val_Notify; END_IF;\n</CData>\n</Line>\n<Line Number=\"484\">\n<CData>\nIF (HiDev.Val_Notify&gt;Wrk_Notify) THEN Wrk_Notify := HiDev.Val_Notify; END_IF;\n</CData>\n</Line>\n<Line Number=\"485\">\n<CData>\nIF (LoDev.Val_Notify&gt;Wrk_Notify) THEN Wrk_Notify := LoDev.Val_Notify; END_IF;\n</CData>\n</Line>\n<Line Number=\"486\">\n<CData>\nIF (LoLoDev.Val_Notify&gt;Wrk_Notify) THEN Wrk_Notify := LoLoDev.Val_Notify; END_IF;\n</CData>\n</Line>\n<Line Number=\"487\">\n<CData>\nIF (IntlkTrip.Val_Notify&gt;Wrk_Notify) THEN Wrk_Notify := IntlkTrip.Val_Notify; END_IF;\n</CData>\n</Line>\n<Line Number=\"488\">\n<CData>\nIF (Fail.Val_Notify&gt;Wrk_Notify) THEN Wrk_Notify := Fail.Val_Notify; END_IF;\n</CData>\n</Line>\n<Line Number=\"489\">\n<CData>\n// ##### V3.1-00: Val_Notify split from reset readies.\n</CData>\n</Line>\n<Line Number=\"490\">\n<CData>\nWrk_Notify.0 := ((HiHiDev.Val_Notify.0 OR HiDev.Val_Notify.0 OR LoDev.Val_Notify.0) OR (LoLoDev.Val_Notify.0 OR IntlkTrip.Val_Notify.0 OR Fail.Val_Notify.0));\n</CData>\n</Line>\n<Line Number=\"491\">\n<CData>\nVal_Notify := Wrk_Notify;\n</CData>\n</Line>\n<Line Number=\"492\">\n<CData>\n// ##### V3.0-03: Interlock shed 'latch' now clears automatically on Interlocks OK.\n</CData>\n</Line>\n<Line Number=\"493\">\n<CData>\nRdy_Reset := ((HiHiDev.Rdy_Reset OR HiDev.Rdy_Reset OR LoDev.Rdy_Reset) OR (LoLoDev.Rdy_Reset OR IntlkTrip.Rdy_Reset OR Fail.Rdy_Reset));\n</CData>\n</Line>\n<Line Number=\"494\">\n<CData>\nRdy_ResetAckAll := ((HiHiDev.Rdy_Ack OR HiDev.Rdy_Ack OR LoDev.Rdy_Ack) OR (LoLoDev.Rdy_Ack OR IntlkTrip.Rdy_Ack OR Fail.Rdy_Ack) OR Rdy_Reset);\n</CData>\n</Line>\n<Line Number=\"495\">\n<CData>\n</CData>\n</Line>\n<Line Number=\"496\">\n<CData>\n//END\n</CData>\n</Line>\n<Line Number=\"497\">\n<CData>\n</CData>\n</Line>\n</STContent>\n</Routine>\n<Routine Name=\"Prescan\" Type=\"RLL\">\n<Description>\n<CData>\nClear Ownership. clear \"lurking\" Commands.\n</CData>\n</Description>\n<RLLContent>\n<Rung Number=\"0\" Type=\"N\">\n<Comment>\n<CData>\n============================================================\nPRESCAN (POWERUP) HANDLING\n============================================================\nOn Prescan, set the Ownership to NO OWNER (0).\n</CData>\n</Comment>\n<Text>\n<CData>\nCLR(PSet_Owner)CLR(Val_Owner);\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"1\" Type=\"N\">\n<Comment>\n<CData>\n\"Lurking\" Program Commands are cleared if they are set as \"Edge\" triggered\n(cleared by this object as received).\n(Lurking Acknowledge Commands are left to perform their action.)\n\n#####  V3.0-05  2014-02-28:  Added Fail alarm to bits cleared  #####\n</CData>\n</Comment>\n<Text>\n<CData>\nXIC(Cfg_PCmdClear)[OTU(PCmd_Casc) OTU(PCmd_Auto) OTU(PCmd_Man) ,OTU(PCmd_Acq) OTU(PCmd_Rel) OTU(PCmd_Lock) OTU(PCmd_Unlock) ,OTU(PCmd_HiHiDevSuppress) OTU(PCmd_HiHiDevUnsuppress) OTU(PCmd_HiDevSuppress) OTU(PCmd_HiDevUnsuppress) ,OTU(PCmd_LoDevSuppress) OTU(PCmd_LoDevUnsuppress) OTU(PCmd_LoLoDevSuppress) OTU(PCmd_LoLoDevUnsuppress) ,OTU(PCmd_IntlkTripSuppress) OTU(PCmd_IntlkTripUnsuppress) OTU(PCmd_FailSuppress) OTU(PCmd_FailUnsuppress) ];\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"2\" Type=\"N\">\n<Comment>\n<CData>\n\"Lurking\" Operator Commands are cleared, as well as the Program Reset Command.\n</CData>\n</Comment>\n<Text>\n<CData>\n[OTU(OCmd_Casc) OTU(OCmd_Auto) OTU(OCmd_Man) ,OTU(OCmd_Bypass) OTU(OCmd_Check) OTU(MCmd_Disable) OTU(MCmd_Enable) ,OTU(OCmd_Reset) OTU(OCmd_ResetAckAll) OTU(PCmd_Reset) ];\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"3\" Type=\"N\">\n<Comment>\n<CData>\nThe Override Command is set to NONE.\n</CData>\n</Comment>\n<Text>\n<CData>\nCLR(Inp_OvrdCmd);\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"4\" Type=\"N\">\n<Comment>\n<CData>\nSince we're not processing Operator Commands or Settings, we're not READY for any of them either!\n</CData>\n</Comment>\n<Text>\n<CData>\n[OTU(Rdy_Casc) OTU(Rdy_Auto) OTU(Rdy_Man) ,OTU(Rdy_Bypass) OTU(Rdy_Check) OTU(Rdy_Disable) OTU(Rdy_Enable) ,OTU(Rdy_Reset) OTU(Rdy_ResetAckAll) ,OTU(Rdy_Ratio) OTU(Rdy_SP) OTU(Rdy_CV) OTU(Rdy_OSet) ];\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"5\" Type=\"N\">\n<Comment>\n<CData>\n============================================================\nPOWERUP LOOP MODE, SP and CV HANDLING\n============================================================\nIf the Powerup Loop Mode is invalid, or if set to Cascade when\nCascade is not configured to exist, set the Powerup Loop Mode to NONE.\n</CData>\n</Comment>\n<Text>\n<CData>\nXIO(Cfg_HasCasc)EQU(Cfg_PwrupLM,3)CLR(Cfg_PwrupLM);\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"6\" Type=\"N\">\n<Comment>\n<CData>\nIf the Powerup Loop Mode is 3, set the Loop Mode to Cascade.\n</CData>\n</Comment>\n<Text>\n<CData>\nXIC(Cfg_HasCasc)EQU(Cfg_PwrupLM,3)OTU(Wrk_Man)OTU(Wrk_Auto)OTL(Wrk_Casc);\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"7\" Type=\"N\">\n<Comment>\n<CData>\nIf the Powerup Loop Mode is 2, set the Loop Mode to Auto.\n</CData>\n</Comment>\n<Text>\n<CData>\nEQU(Cfg_PwrupLM,2)OTU(Wrk_Man)OTL(Wrk_Auto)OTU(Wrk_Casc);\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"8\" Type=\"N\">\n<Comment>\n<CData>\nIf the Powerup Loop Mode is 1, set the Loop Mode to Manual.\n</CData>\n</Comment>\n<Text>\n<CData>\nEQU(Cfg_PwrupLM,1)OTL(Wrk_Man)OTU(Wrk_Auto)OTU(Wrk_Casc);\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"9\" Type=\"N\">\n<Comment>\n<CData>\nIf the Powerup Loop Mode is 0, the Mode, CV, and SP are left in their\nlast (powerdown) states.\n\nIf the Powerup Loop Mode is GREATER THAN 0 (1=Man, 2=Auto, 3=Casc),\nthen set the initial values of SP and CV.\n\n(There are several places to put each of these:  Override Inputs,\nProgram Settings, Operator Settings, plus the Input for the Cascade Mode SP.)\n</CData>\n</Comment>\n<Text>\n<CData>\nGRT(Cfg_PwrupLM,0)[[[XIO(Cfg_HasRatio) ,EQU(Val_Ratio,0.0) ] MOV(Cfg_PwrupSP,Inp_CascSP) ,XIC(Cfg_HasRatio) NEQ(Val_Ratio,0.0) DIV(Cfg_PwrupSP,Val_Ratio,Inp_CascSP) ] ,MOV(Cfg_PwrupSP,PSet_SP) MOV(Cfg_PwrupSP,OSet_SP) MOV(Cfg_PwrupSP,Inp_OvrdSP) ,MOV(Cfg_PwrupSP,Val_SP) MOV(Cfg_PwrupCV,PSet_CV) MOV(Cfg_PwrupCV,OSet_CV) ,MOV(Cfg_PwrupCV,Inp_OvrdCV) MOV(Cfg_PwrupCV,Inp_HandFdbk) MOV(Cfg_PwrupCV,Val_CVOut) MOV(Cfg_PwrupCV,Inp_CVPrev) ,CPT(Inp_CVInitVal,Cfg_PwrupCV/100.0*(Cfg_CVEUMax-Cfg_CVEUMin)+Cfg_CVEUMin) ];\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"10\" Type=\"N\">\n<Comment>\n<CData>\nIf the Powerup Loop Mode was zero, the loop mode must be\nchecked for consistency (only one Wrk_&lt;mode&gt; bit set).\nThis rung makes sure the internal mode bits are consistent in case\nthey were not set above and somehow NO, or MULTIPLE\nloop mode internal latches got set.\nOnly one can be set at a time.  Manual wins, then Auto...\n</CData>\n</Comment>\n<Text>\n<CData>\n[[XIC(Wrk_Man) OTU(Wrk_Auto) ,XIC(Wrk_Auto) ] OTU(Wrk_Casc) ,XIO(Wrk_Man) XIO(Wrk_Auto) XIO(Wrk_Casc) OTL(Wrk_Man) ];\n</CData>\n</Text>\n</Rung>\n<Rung Number=\"11\" Type=\"N\">\n<Comment>\n<CData>\nThis rung sets the PIDE to attempt to acquire the Autotune on first scan.\nIf Autotune is acquired, the GainTooHigh and GainTooLow bits will be turned off --\nin this case, the HMI_HasAutotune flag is left ON.\nIf Autotune is NOT acquired, the GainTooHigh and GainTooLow bits will both be left on\n(and in normal autotune operation, they will never BOTH be set).\nIf they are both on, the Autotune tag has NOT been added to the\nPIDE sub-AOI, and the HMI_HasAutotune flag is turned OFF.\n\n#####  V3.0-05 2014-02-28: Added ability for user to modify to include Autotune  #####\n</CData>\n</Comment>\n<Text>\n<CData>\nOTL(HMI_HasAutotune)OTL(Wrk_Autotune.GainTooLarge)OTL(Wrk_Autotune.GainTooSmall)OTL(Wrk_PIDE.AtuneAcquire);\n</CData>\n</Text>\n</Rung>\n</RLLContent>\n</Routine>\n</Routines>\n<Dependencies>\n<Dependency Name=\"STRING_NavTag\" Type=\"DataType\"/>\n<Dependency Name=\"STRING_8\" Type=\"DataType\"/>\n<Dependency Name=\"STRING_40\" Type=\"DataType\"/>\n<Dependency Name=\"STRING_20\" Type=\"DataType\"/>\n<Dependency Name=\"P_Alarm\" Type=\"AddOnInstructionDefinition\"/>\n<Dependency Name=\"P_Gate\" Type=\"AddOnInstructionDefinition\"/>\n<Dependency Name=\"STRING_12\" Type=\"DataType\"/>\n<Dependency Name=\"P_Mode\" Type=\"AddOnInstructionDefinition\"/>\n<Dependency Name=\"P_PIDE_only\" Type=\"AddOnInstructionDefinition\"/>\n</Dependencies>\n</AddOnInstructionDefinition>"